//print numbrs in increasing orders
public class numberincreasing {
    public static void increasing(int n)
    {
        if(n==1)
        {
            System.out.print(n+" ");
            return;
        }
        increasing(n-1);
        System.out.print(n+" ");
    }
    public static void main(String[] args) {
        int n=5;
        increasing(n);
    }
    
}
